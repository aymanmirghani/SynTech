  <DataSets>
    <DataSet Name="PurchasedProducts_ProductSalesByCustomer">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>PurchasedProducts</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>ProductSalesByCustomerTableAdapter</rd:TableAdapterName>
        <rd:TableName>ProductSalesByCustomer</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     Customer.Name AS Customer, SalesTerritory.Name AS Territory, SUM(SalesInvoiceDetail.Quantity) AS Quantity, 
                      SUM(SalesInvoiceDetail.UnitPrice * SalesInvoiceDetail.Quantity) AS Amount
FROM         SalesInvoiceDetail INNER JOIN
                      Product ON SalesInvoiceDetail.ProductID = Product.ProductID INNER JOIN
                      SalesInvoiceHeader ON SalesInvoiceDetail.InvoiceID = SalesInvoiceHeader.InvoiceID CROSS JOIN
                      SalesTerritory INNER JOIN
                      Customer ON SalesTerritory.TerritoryID = Customer.TerritoryID
WHERE     (SalesInvoiceHeader.InvoiceDate BETWEEN @FromDate AND @ToDate) AND (Product.ProductID = @ProductID)
GROUP BY Customer.Name, SalesTerritory.Name</CommandText>
        <DataSourceName>mics_serverConnectionString</DataSourceName>
      </Query>
      <Fields>
        <Field Name="Customer">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Customer</DataField>
        </Field>
        <Field Name="Territory">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Territory</DataField>
        </Field>
        <Field Name="Quantity">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Quantity</DataField>
        </Field>
        <Field Name="Amount">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Amount</DataField>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
