  <DataSets>
    <DataSet Name="PurchasedProducts_MonthlyPurchased">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>PurchasedProducts</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>MonthlyPurchasedTableAdapter</rd:TableAdapterName>
        <rd:TableName>MonthlyPurchased</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     Product.Name, SUM(PurchaseInvoiceDetail.Quantity) AS Quantity, SUM(PurchaseInvoiceDetail.UnitPrice * PurchaseInvoiceDetail.Quantity) 
                      AS Price
FROM         Product INNER JOIN
                      PurchaseInvoiceDetail ON Product.ProductID = PurchaseInvoiceDetail.ProductID INNER JOIN
                      PurchaseInvoiceHeader ON PurchaseInvoiceDetail.InvoiceID = PurchaseInvoiceHeader.InvoiceID
WHERE     (PurchaseInvoiceHeader.Status = 1) AND (PurchaseInvoiceHeader.InvoiceDate BETWEEN @dateFrom AND @dateTo)
GROUP BY Product.Name</CommandText>
        <DataSourceName>mics_serverConnectionString</DataSourceName>
      </Query>
      <Fields>
        <Field Name="Name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Name</DataField>
        </Field>
        <Field Name="Price">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Price</DataField>
        </Field>
        <Field Name="Quantity">
          <rd:TypeName>System.Int64</rd:TypeName>
          <DataField>Quantity</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PurchasedProducts_ChartProductPurchased">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>PurchasedProducts</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>ChartProductPurchasedTableAdapter</rd:TableAdapterName>
        <rd:TableName>ChartProductPurchased</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     ProductCategory.Name AS Category, SUM(PurchaseInvoiceDetail.Quantity) AS Quantity, 
                      SUM(PurchaseInvoiceDetail.UnitPrice * PurchaseInvoiceDetail.Quantity) AS Price
FROM         Product INNER JOIN
                      ProductSubcategory ON Product.ProductSubcategoryID = ProductSubcategory.ProductSubcategoryID INNER JOIN
                      PurchaseInvoiceDetail ON Product.ProductID = PurchaseInvoiceDetail.ProductID INNER JOIN
                      ProductCategory ON ProductSubcategory.ProductCategoryID = ProductCategory.ProductCategoryID INNER JOIN
                      PurchaseInvoiceHeader ON PurchaseInvoiceDetail.InvoiceID = PurchaseInvoiceHeader.InvoiceID
WHERE     (PurchaseInvoiceHeader.Status = 1) AND (PurchaseInvoiceHeader.InvoiceDate BETWEEN @FromDate AND @ToDate)
GROUP BY ProductCategory.Name</CommandText>
        <DataSourceName>mics_serverConnectionString</DataSourceName>
      </Query>
      <Fields>
        <Field Name="Category">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Category</DataField>
        </Field>
        <Field Name="Quantity">
          <rd:TypeName>System.Int64</rd:TypeName>
          <DataField>Quantity</DataField>
        </Field>
        <Field Name="Price">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Price</DataField>
        </Field>
      </Fields>
    </DataSet>
