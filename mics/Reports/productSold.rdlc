  <DataSets>
    <DataSet Name="PurchasedProducts_ProductSold">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>PurchasedProducts</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>ProductSoldTableAdapter</rd:TableAdapterName>
        <rd:TableName>ProductSold</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     Product.Name, SUM(SalesInvoiceDetail.Quantity) AS Quantity, SUM(SalesInvoiceDetail.UnitPrice * SalesInvoiceDetail.Quantity) AS Price
FROM         Product INNER JOIN
                      SalesInvoiceDetail ON Product.ProductID = SalesInvoiceDetail.ProductID INNER JOIN
                      SalesInvoiceHeader ON SalesInvoiceDetail.InvoiceID = SalesInvoiceHeader.InvoiceID
WHERE     (SalesInvoiceHeader.Status = 1) AND (SalesInvoiceHeader.DueDate BETWEEN @FromDate AND @ToDate)
GROUP BY Product.Name</CommandText>
        <DataSourceName>mics_serverConnectionString</DataSourceName>
      </Query>
      <Fields>
        <Field Name="Name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Name</DataField>
        </Field>
        <Field Name="Quantity">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Quantity</DataField>
        </Field>
        <Field Name="Price">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Price</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PurchasedProducts_ChartProductSold">
      <rd:DataSetInfo>
        <rd:TableAdapterGetDataMethod>GetData</rd:TableAdapterGetDataMethod>
        <rd:DataSetName>PurchasedProducts</rd:DataSetName>
        <rd:TableAdapterFillMethod>Fill</rd:TableAdapterFillMethod>
        <rd:TableAdapterName>ChartProductSoldTableAdapter</rd:TableAdapterName>
        <rd:TableName>ChartProductSold</rd:TableName>
      </rd:DataSetInfo>
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     ProductCategory.Name AS Category, SUM(SalesInvoiceDetail.Quantity) AS Quantity, SUM(SalesInvoiceDetail.UnitPrice * SalesInvoiceDetail.Quantity) 
                      AS Price
FROM         Product INNER JOIN
                      ProductSubcategory ON Product.ProductSubcategoryID = ProductSubcategory.ProductSubcategoryID INNER JOIN
                      SalesInvoiceDetail ON Product.ProductID = SalesInvoiceDetail.ProductID INNER JOIN
                      SalesInvoiceHeader ON SalesInvoiceDetail.InvoiceID = SalesInvoiceHeader.InvoiceID INNER JOIN
                      ProductCategory ON ProductSubcategory.ProductCategoryID = ProductCategory.ProductCategoryID
WHERE     (SalesInvoiceHeader.Status = 1) AND (SalesInvoiceHeader.DueDate BETWEEN @FromDate AND @ToDate)
GROUP BY ProductCategory.Name</CommandText>
        <DataSourceName>mics_serverConnectionString</DataSourceName>
      </Query>
      <Fields>
        <Field Name="Category">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Category</DataField>
        </Field>
        <Field Name="Quantity">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Quantity</DataField>
        </Field>
        <Field Name="Price">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Price</DataField>
        </Field>
      </Fields>
    </DataSet>
